{
  "version": 2,

  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    },
    { "src": "api/server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/.*", "dest": "build/index.html" },
    {
      "src": "/contacts",
      "dest": "/api/server.js"
    }
  ]
}
