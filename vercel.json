{
  "version": 2,

  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    },
    { "src": "api/server.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/contacts",
      "handle": "/api/server.js"
    },

    { "src": "/.*", "dest": "/build/index.html" }
  ]
}
